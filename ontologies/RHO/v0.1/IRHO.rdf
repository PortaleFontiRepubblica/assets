            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/italia/republic-history/onto/org/hasOrgType"/>
                <owl:hasValue rdf:resource="https://w3id.org/italia/republic-history/onto/org/Cabinet"/>
